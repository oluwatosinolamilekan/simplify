{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "awobaz/compoships": "^2.1",
        "bensampo/laravel-enum": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "protoqol/prequel": "^1.22",
        "richan-fongdasen/eloquent-blameable": "^1.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "install-tools": [
            "php artisan prequel:install",
            "php artisan telescope:install",
            "php artisan migrate"
        ],
        "start": "php artisan serve",
        "fix": "./vendor/bin/php-cs-fixer --config=.php_cs_fixer.php --allow-risky=yes fix",
        "stan": "php ./vendor/bin/phpstan analyze --memory-limit=10 -l 1",
        "migrate": "php artisan migrate",
        "migrate:rollback": "php artisan migrate:rollback",
        "migration:make": "php artisan migration:make"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
